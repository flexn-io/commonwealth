{
    "compilerOptions": {
        "declaration": true,
        "resolveJsonModule": true,
        "target": "es5",
        "jsx": "react",
        "module": "commonjs",
        "moduleResolution": "node",
        "importHelpers": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "declarationMap": true,
        "noImplicitAny": true,
    }
}